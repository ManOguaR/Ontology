        </argushiigi:usesLabelPattern>
        <argushiigi:usesValuePattern>
          <argushiigi:Pattern>
            <argushiigi:nextPattern>
              <argushiigi:Pattern>
                <argushiigi:nextPattern>
                  <argushiigi:Pattern>
                    <argushiigi:nextPattern>
                      <argushiigi:Pattern>
