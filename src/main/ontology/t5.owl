  </owl:Class>
  <owl:Class rdf:ID="Race">
    <rdfs:label xml:lang="en">Race</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Template"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment xml:lang="en">The spieces of creature to which a sophont belongs.

Races act as templates, giving typical features and capabilities for a member of that race.
</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://wiki.travellerrpg.com/Races"/>
    <rdfs:isDefinedBy rdf:resource="http://data.travellerrpg.com/publication/traveller-5/core"/>
  </owl:Class>
  <owl:Class rdf:ID="Scale">
    <rdfs:label xml:lang="en">Scale</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment xml:lang="en">A level of size, distance, distribution, etc.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#C5">
    <rdfs:label xml:lang="en">C4</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
    <rdfs:comment xml:lang="en">An abstract abilitty indicating aquired knowledge.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="MajorRace">
    <rdfs:label xml:lang="en">Major Race</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Race"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="MinorRace"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">Generally defined as a race that developed jump drive technology independently.</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://wiki.travellerrpg.com/Major_Race"/>
    <rdfs:isDefinedBy rdf:resource="http://data.travellerrpg.com/publication/traveller-5/core"/>
  </owl:Class>
  <owl:Class rdf:ID="World">
    <rdfs:label xml:lang="en">World</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Place"/>
    <rdfs:comment xml:lang="en">A planet</rdfs:comment>
    <skos:scopeNote xml:lang="en">Generally, a world refers to the main inhabited planet of a star system</skos:scopeNote>
  </owl:Class>
  <owl:Class rdf:ID="DiceConventions"/>
  <owl:Class rdf:about="#C3">
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
    <rdfs:comment xml:lang="en">An abstract characteristic reprenting physical robustness.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Grace">
    <rdfs:label xml:lang="en">Grace</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C2"/>
    <geneticProfileLabel xml:lang="en">G</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Gra</skos:prefLabel>
    <rdfs:comment xml:lang="en">Whole of body fluidity</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Fame">
    <rdfs:comment xml:lang="en">The scale of an entity's fame.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Scale"/>
    <rdfs:label xml:lang="en">Fame Level</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Feature"/>
  </owl:Class>
  <owl:Class rdf:ID="Subsector">
    <rdfs:label xml:lang="en">Subsector</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Place"/>
    <rdfs:comment xml:lang="en">A sub-sector of space covering approximately 10pc</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="DistanceBand">
    <rdfs:subClassOf rdf:resource="#Scale"/>
  </owl:Class>
  <owl:Class rdf:ID="Endurance">
    <rdfs:label xml:lang="en">Endurance</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C3"/>
    <geneticProfileLabel xml:lang="en">E</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">End</skos:prefLabel>
    <rdfs:comment xml:lang="en">Resistance to fatigue</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="KnowledgeType">
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#SkillType"/>
    <rdfs:label xml:lang="en">Knowledge Type </rdfs:label>
    <rdfs:comment xml:lang="en">A knowledge represents a restricted form of skill in a specific area.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://data.travellerrpg.com/publication/traveller-5/core"/>
  </owl:Class>
  <owl:Class rdf:ID="Load">
    <rdfs:label xml:lang="en">Load</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#EntityState"/>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Mass"/>
    <rdfs:comment xml:lang="en">The unmodified load carried by something</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#C1">
    <rdfs:label xml:lang="en">C1</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
