  </owl:Class>
  <owl:Class rdf:ID="SkillType">
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#SkillType"/>
    <rdfs:label xml:lang="en">Skill Type </rdfs:label>
    <rdfs:comment xml:lang="en">A Traveller 5 skill</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Dexterity">
    <rdfs:label xml:lang="en">Dexterity</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="C2"/>
    </rdfs:subClassOf>
    <geneticProfileLabel xml:lang="en">D</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Dex</skos:prefLabel>
    <rdfs:comment xml:lang="en">Fine motor control</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#C2">
    <rdfs:label xml:lang="en">C2</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
    <rdfs:comment xml:lang="en">An abstract attribute denoting physical motor control</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Instinct">
    <rdfs:label xml:lang="en">Instinct</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="C5"/>
    </rdfs:subClassOf>
    <geneticProfileLabel xml:lang="en">I</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Ins</skos:prefLabel>
    <rdfs:comment xml:lang="en">Skills and knowledge acquired from genetic heritage.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="CP">
    <rdfs:label xml:lang="en">CP</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
    <rdfs:comment xml:lang="en">An abstract characteristic describing mental power.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Sanity">
    <rdfs:label xml:lang="en">Sanity</rdfs:label>
    <rdfs:subClassOf rdf:resource="#CS"/>
    <rdfs:comment xml:lang="en">Mental health and stability</rdfs:comment>
    <geneticProfileLabel xml:lang="en">S</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">San</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="PersonalType">
    <rdfs:isDefinedBy rdf:resource="http://data.travellerrpg.com/publication/traveller-5/core"/>
    <rdfs:comment xml:lang="en">A personal skill in interacting with others</rdfs:comment>
    <rdfs:label xml:lang="en">Personal Skill Type </rdfs:label>
    <rdfs:subClassOf rdf:resource="#SkillType"/>
  </owl:Class>
  <owl:Class rdf:ID="Agility">
    <rdfs:label xml:lang="en">Agility</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C2"/>
    <geneticProfileLabel xml:lang="en">A</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Agi</skos:prefLabel>
    <rdfs:comment xml:lang="en">Body coordination</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Strength">
    <rdfs:label xml:lang="en">Strength</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="C1"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Physical power</rdfs:comment>
    <geneticProfileLabel xml:lang="en">S</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Str</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="Stamina">
    <rdfs:label xml:lang="en">Stamina</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="C3"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Long-term persistence</rdfs:comment>
    <geneticProfileLabel xml:lang="en">S</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Sta</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="Vigor">
    <rdfs:label xml:lang="en">Vigor</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#C3"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Short term resistence to fatigue</rdfs:comment>
    <geneticProfileLabel xml:lang="en">V</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Vig</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="Sector">
    <rdfs:comment xml:lang="en">A sector of space covering approximately 40pc</rdfs:comment>
    <rdfs:label xml:lang="en">Sector </rdfs:label>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Place">
        <owl:disjointWith>
          <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Event">
            <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Place"/>
            <owl:disjointWith>
              <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Asset">
                <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Place"/>
                <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Event"/>
                <owl:disjointWith>
                  <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Actor">
                    <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Place"/>
                    <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Event"/>
                    <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Asset"/>
                  </rdf:Description>
                </owl:disjointWith>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Actor"/>
          </rdf:Description>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Asset"/>
        <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Actor"/>
      </rdf:Description>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Psionics">
    <rdfs:label xml:lang="en">Psionics</rdfs:label>
    <rdfs:subClassOf rdf:resource="#CP"/>
    <rdfs:comment xml:lang="en">Extra-sensory mental ability</rdfs:comment>
    <geneticProfileLabel xml:lang="en">P</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Psi</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="Education">
    <rdfs:label xml:lang="en">Education</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#C5"/>
    </rdfs:subClassOf>
    <geneticProfileLabel xml:lang="en">E</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Edu</skos:prefLabel>
    <rdfs:comment xml:lang="en">Skills and knowledge acquired from formall education</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TalentType">
    <rdfs:label xml:lang="en">Talent Type</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#FacultyType"/>
