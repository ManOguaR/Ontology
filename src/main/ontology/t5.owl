    <rdfs:comment xml:lang="en">A race that is not a major race.</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://wiki.travellerrpg.com/Minor_Race"/>
    <rdfs:isDefinedBy rdf:resource="http://data.travellerrpg.com/publication/traveller-5/core"/>
  </owl:Class>
  <owl:Class rdf:ID="Training">
    <rdfs:label xml:lang="en">Training</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C5"/>
    <rdfs:comment xml:lang="en">Skills acquired from cultural background</rdfs:comment>
    <geneticProfileLabel xml:lang="en">T</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Tra</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="WorldKnowledgeType">
    <rdfs:subClassOf rdf:resource="#KnowledgeType"/>
    <rdfs:label xml:lang="en">World Knowledge </rdfs:label>
    <rdfs:comment xml:lang="en">Knowledge associated with a specific world</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Intelligence">
    <rdfs:label xml:lang="en">Intelligence</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C4"/>
    <rdfs:comment xml:lang="en">Ability to think and reason</rdfs:comment>
    <geneticProfileLabel xml:lang="en">I</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Int</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="SocialStanding">
