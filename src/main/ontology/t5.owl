<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:rpg="http://data.travellerrpg.com/ontology/rpg#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns="http://data.travellerrpg.com/ontology/t5#"
  xml:base="http://data.travellerrpg.com/ontology/t5">
  <owl:Ontology rdf:about="">
    <rdfs:comment xml:lang="en">The core Traveller 5 ontology.

This ontology provides upper-level concepts for Traveller 5. Detailed information on specific areas, such as characters or worlds can be found in subsiduary ontologies.</rdfs:comment>
    <rdfs:seeAlso xml:lang="en">http://www.travellerrpg.com/CotI/Discuss/showthread.php?t=30041</rdfs:seeAlso>
    <owl:imports rdf:resource="http://data.travellerrpg.com/ontology/rpg"/>
    <terms:license>
      <terms:RightsStatement rdf:about="http://wiki.travellerrpg.com/Copyright">
        <rdfs:label>Traveller Copyright</rdfs:label>
      </terms:RightsStatement>
    </terms:license>
    <rdfs:isDefinedBy>
      <rpg:Rules rdf:about="http://data.travellerrpg.com/publication/traveller-5/core">
        <rdfs:comment xml:lang="en">The Traveller 5 core rules</rdfs:comment>
        <rdfs:label xml:lang="en">Traveller 5 Core</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
      </rpg:Rules>
    </rdfs:isDefinedBy>
  </owl:Ontology>
  <owl:Class rdf:ID="CS">
    <rdfs:label xml:lang="en">CS</rdfs:label>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Attribute">
        <skos:prefLabel xml:lang="en">Characteristic</skos:prefLabel>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">An abstract characteristic describing mental stability.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="C6">
    <rdfs:label xml:lang="en">C6</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
    <rdfs:comment xml:lang="en">An abstract ability indicating social position or standing.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Charisma">
    <rdfs:label xml:lang="en">Charisma</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C6"/>
    <geneticProfileLabel xml:lang="en">C</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Cha</skos:prefLabel>
    <rdfs:comment xml:lang="en">Position within a small group hierarchy.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="C4">
    <rdfs:label xml:lang="en">C4</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
