    <geneticProfileLabel xml:lang="en">S</geneticProfileLabel>
    <skos:prefLabel xml:lang="en">Soc</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:ID="IntuitionType">
    <rdfs:comment xml:lang="en">An intuitive skill in choosing a course of action without formal evaluation</rdfs:comment>
    <rdfs:label xml:lang="en">Intuitive Skill Type</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://data.travellerrpg.com/publication/traveller-5/core"/>
    <rdfs:subClassOf rdf:resource="#SkillType"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasCP">
    <rdfs:domain>
      <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Character">
        <owl:disjointWith>
          <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Organisation">
            <owl:disjointWith>
              <rdf:Description rdf:about="http://data.travellerrpg.com/ontology/rpg#Extra">
                <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Organisation"/>
                <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Character"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Character"/>
          </rdf:Description>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="http://data.travellerrpg.com/ontology/rpg#Extra"/>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:label xml:lang="en">has CP</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#hasAttribute"/>
    <rdfs:range rdf:resource="#CP"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInstinct">
    <rdfs:label xml:lang="en">has instinct</rdfs:label>
    <rdfs:range rdf:resource="#Instinct"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasC5"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="characteristicGenerator">
    <rdfs:label xml:lang="en">characteristic generator</rdfs:label>
    <skos:scopeNote xml:lang="en">Intended for template characteristics that have a typical value and a range of possiblities.</skos:scopeNote>
    <rdfs:comment xml:lang="en">The means by which a characteristic is generated.</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.travellerrpg.com/ontology/rpg#Attribute"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasEducation">
    <rdfs:label xml:lang="en">has education</rdfs:label>
    <rdfs:range rdf:resource="#Education"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasC5"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCaste">
    <rdfs:label xml:lang="en">has caste</rdfs:label>
    <rdfs:range rdf:resource="#Caste"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasC6"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="knowledgeOf">
    <rdfs:label xml:lang="en">knowledge of </rdfs:label>
    <rdfs:domain rdf:resource="#KnowledgeType"/>
    <rdfs:comment xml:lang="en">Knowledge of something specific</rdfs:comment>
    <skos:scopeNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The knowledge of property can be linked to almost any class or instance of a game entity, setting, skill etc. It alllows knowledges to link to the subject of knowlege</skos:scopeNote>
    <skos:altLabel xml:lang="en">knowledge subject</skos:altLabel>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasVigor">
    <rdfs:range rdf:resource="#Vigor"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasC3"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasC5">
