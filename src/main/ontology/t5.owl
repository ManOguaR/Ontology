    <rdfs:subClassOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#Mass"/>
    <rdfs:comment xml:lang="en">The modified load carried by something.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Caste">
    <rdfs:label xml:lang="en">Caste</rdfs:label>
    <rdfs:subClassOf rdf:resource="#C6"/>
    <skos:prefLabel xml:lang="en">Cas</skos:prefLabel>
    <geneticProfileLabel xml:lang="en">K</geneticProfileLabel>
