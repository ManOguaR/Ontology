    <rdfs:label xml:lang="en">hasC5</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://data.travellerrpg.com/ontology/rpg#hasAttribute"/>
    <rdfs:range rdf:resource="#C5"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasC3">
