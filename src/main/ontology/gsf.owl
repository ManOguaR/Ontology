    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.travellerrpg.com/ontology/gsf#characteristic -->

    <owl:DatatypeProperty rdf:about="&gsf2;characteristic">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">characteristic</rdfs:label>
        <rdfs:comment xml:lang="en">The leve of a characteristic (0-100)</rdfs:comment>
        <rdfs:domain rdf:resource="&gsf2;GSFAttribute"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;value"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.travellerrpg.com/ontology/t5#generator -->

    <owl:DatatypeProperty rdf:about="&t5;generator">
        <rdfs:label xml:lang="en">Generator</rdfs:label>
        <rdfs:comment xml:lang="en">A description of how to generate something</rdfs:comment>
        <rdfs:domain rdf:resource="&gsf2;GSFAttribute"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


