        <skos:prefLabel xml:lang="en">Sta</skos:prefLabel>
        <rdfs:range rdf:resource="&gsf2;Stamina"/>
        <rdfs:domain rdf:resource="&rpg;Actor"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasStrength -->

    <owl:ObjectProperty rdf:about="&gsf2;hasStrength">
        <rdfs:label xml:lang="en">strength</rdfs:label>
