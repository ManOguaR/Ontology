    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="&terms;creator"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasAgility -->

    <owl:ObjectProperty rdf:about="&gsf2;hasAgility">
        <rdfs:label xml:lang="en">agility</rdfs:label>
        <skos:prefLabel xml:lang="en">Agl</skos:prefLabel>
        <rdfs:comment xml:lang="en">Gives the agility of a character</rdfs:comment>
        <rdfs:range rdf:resource="&gsf2;Agility"/>
        <rdfs:domain rdf:resource="&rpg;Actor"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasDamage -->

    <owl:ObjectProperty rdf:about="&gsf2;hasDamage">
        <rdfs:label xml:lang="en">Damage</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the current damage level of the game entity.</rdfs:comment>
        <rdfs:range rdf:resource="&gsf;Damage"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasState"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasDexterity -->

    <owl:ObjectProperty rdf:about="&gsf2;hasDexterity">
        <rdfs:label xml:lang="en">dexterity</rdfs:label>
        <skos:prefLabel xml:lang="en">Dex</skos:prefLabel>
        <rdfs:comment xml:lang="en">Gives the desterity of a character</rdfs:comment>
        <rdfs:range rdf:resource="&gsf2;Dexterity"/>
        <rdfs:domain rdf:resource="&rpg;Actor"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasDurability -->

    <owl:ObjectProperty rdf:about="&gsf2;hasDurability">
        <rdfs:label xml:lang="en">durabilty</rdfs:label>
        <skos:prefLabel xml:lang="en">Dur</skos:prefLabel>
        <rdfs:comment xml:lang="en">The durability of the individual</rdfs:comment>
        <rdfs:range rdf:resource="&gsf2;Durability"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasIntelligence -->

    <owl:ObjectProperty rdf:about="&gsf2;hasIntelligence">
        <rdfs:label xml:lang="en">intelligence</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the intelligence of a character</rdfs:comment>
        <skos:prefLabel xml:lang="en">Int</skos:prefLabel>
        <rdfs:range rdf:resource="&gsf2;Intelligence"/>
        <rdfs:domain rdf:resource="&rpg;Actor"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/gsf#hasStamina -->

    <owl:ObjectProperty rdf:about="&gsf2;hasStamina">
        <rdfs:label xml:lang="en">stamina</rdfs:label>
