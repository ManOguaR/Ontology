    <rdfs:label xml:lang="en">Open Publication License v1.0</rdfs:label>
  </terms:LicenseDocument>
  <DateSystem rdf:about="http://placetime.com/instant/gregorian/">
    <referenceMaterial rdf:resource="http://en.wikipedia.org/wiki/Gregorian_calendar"/>
    <skos:scopeNote xml:lang="en">This calendar is generally used in an RPG setting to provide a parallel calendar to the game calendar.</skos:scopeNote>
    <rdfs:comment xml:lang="en">Dates represented by the Gregorian Calendar</rdfs:comment>
    <rdfs:label xml:lang="en">Gregorian Calendar</rdfs:label>
  </DateSystem>
