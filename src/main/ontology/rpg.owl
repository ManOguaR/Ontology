        <rdfs:comment xml:lang="en">A unit of time (duration)</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Unit -->

    <owl:Class rdf:about="&rpg;Unit">
        <rdfs:label xml:lang="en">Unit</rdfs:label>
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en">A unit of measure</rdfs:comment>
        <skos:editorialNote xml:lang="en">Resist the tempation to produce a fully fledged system of units and measures. That way lies madness.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This heirarchy is a small unit model designed for use within RPGs. Specific types of units are modelled as subclasses.</skos:scopeNote>
