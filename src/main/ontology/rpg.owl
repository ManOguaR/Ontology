<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY terms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY cc "http://creativecommons.org/ns#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rpg "http://data.travellerrpg.com/ontology/rpg#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://data.travellerrpg.com/ontology/rpg#"
     xml:base="http://data.travellerrpg.com/ontology/rpg"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:cc="http://creativecommons.org/ns#"
     xmlns:rpg="http://data.travellerrpg.com/ontology/rpg#">
    <owl:Ontology rdf:about="http://data.travellerrpg.com/ontology/rpg">
        <rdfs:label xml:lang="en">Generic Role Playing Game Ontology</rdfs:label>
        <skos:scopeNote xml:lang="en">RPGs represent imaginary situations.

Data available on the semantic web represent a wide variety of resources, from the wholely imaginary to the completely real.

This ontology imports SKOS for knowledge descriptions and FOAF for player information. Otherwise, it keeps the real world at a distance, so that difficulties with software confusing a cyberpunk description of London and the real London are kept to a minimum.</skos:scopeNote>
        <rdfs:comment xml:lang="en">The RPG ontology contains abstract descriptions of common RPG concepts, such as Characters, Skills, NPCs etc. A specigic RPG will specialise these concepts to fit the RPG.
</rdfs:comment>
        <terms:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
        <rdfs:seeAlso rdf:resource="http://www.travellerrpg.com/CotI/Discuss/showthread.php?t=30041"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <terms:creator rdf:resource="https://github.com/charvolant"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.travellerrpg.com/ontology/rpg#referenceMaterial -->

    <owl:AnnotationProperty rdf:about="&rpg;referenceMaterial">
        <rdfs:label xml:lang="en">reference material</rdfs:label>
        <rdfs:comment xml:lang="en">A reference to a document or concept that contains additional information.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rdfs;seeAlso"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="&terms;license"/>
    


    <!-- http://purl.org/dc/terms/rights -->

    <owl:AnnotationProperty rdf:about="&terms;rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.travellerrpg.com/ontology/rpg#fromUnit -->

    <owl:ObjectProperty rdf:about="&rpg;fromUnit">
        <rdfs:label xml:lang="en">from unit</rdfs:label>
        <rdfs:domain rdf:resource="&rpg;Conversion"/>
        <rdfs:range rdf:resource="&rpg;Unit"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasAttribute -->

    <owl:ObjectProperty rdf:about="&rpg;hasAttribute">
        <rdfs:label xml:lang="en">has attribute</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the attributes that describe an actor</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Attribute"/>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasFaculty -->

    <owl:ObjectProperty rdf:about="&rpg;hasFaculty">
        <rdfs:label xml:lang="en">has faculty</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an actor has a specific faculty available</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Faculty"/>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasFeature -->

    <owl:ObjectProperty rdf:about="&rpg;hasFeature">
        <rdfs:label xml:lang="en">has feature</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that a character possesses a certain feature.</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Feature"/>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasMass -->

    <owl:ObjectProperty rdf:about="&rpg;hasMass">
        <rdfs:label xml:lang="en">mass</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the basic mass of a game item.</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Mass"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasPrice -->

    <owl:ObjectProperty rdf:about="&rpg;hasPrice">
        <rdfs:label xml:lang="en">price</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the price of an item.

If left further unspecified, this generallty means the common, in-game price.</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Price"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasAttribute"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasSense -->

    <owl:ObjectProperty rdf:about="&rpg;hasSense">
        <rdfs:label xml:lang="en">has sense</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an actor has a specific sense available</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
        <rdfs:range rdf:resource="&rpg;Sense"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasSetting -->

    <owl:ObjectProperty rdf:about="&rpg;hasSetting">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">has setting</rdfs:label>
        <rdfs:comment xml:lang="en">Indiicates that a gane entity, state or template applies to or comes from a particular setting.</rdfs:comment>
        <skos:scopeNote xml:lang="en">Settings themselves may have parent settings.</skos:scopeNote>
        <rdfs:range rdf:resource="&rpg;Setting"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasSkill -->

    <owl:ObjectProperty rdf:about="&rpg;hasSkill">
        <rdfs:label xml:lang="en">has skill</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an actor has a level of a specific skill</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
        <rdfs:range rdf:resource="&rpg;Skill"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;hasFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#hasState -->

    <owl:ObjectProperty rdf:about="&rpg;hasState">
        <rdfs:label xml:lang="en">is in state</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that a game item is currently in a particular state</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
        <rdfs:range rdf:resource="&rpg;State"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#inFacultyType -->

    <owl:ObjectProperty rdf:about="&rpg;inFacultyType">
        <rdfs:label xml:lang="en">in faculty</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the type of faculty that a faculty level refers to.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rpg;inFeatureType"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#inFeatureType -->

    <owl:ObjectProperty rdf:about="&rpg;inFeatureType">
        <rdfs:label xml:lang="en">in feature type</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that a feature is associated with a particular type of feature concept.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Feature"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#inSenseType -->

    <owl:ObjectProperty rdf:about="&rpg;inSenseType">
        <rdfs:label xml:lang="en">in sense</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the speciifc sense that a sense level gives the capability level of.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Sense"/>
        <rdfs:range rdf:resource="&rpg;SenseType"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;inFeatureType"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#inSkillType -->

    <owl:ObjectProperty rdf:about="&rpg;inSkillType">
        <rdfs:label xml:lang="en">in skill</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that a skill level is in a specific skill.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Skill"/>
        <rdfs:range rdf:resource="&rpg;SkillType"/>
        <rdfs:subPropertyOf rdf:resource="&rpg;inFeatureType"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#isPortrayedBy -->

    <owl:ObjectProperty rdf:about="&rpg;isPortrayedBy">
        <rdfs:label xml:lang="en">is portrayed by</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that a character is portrayed by a specific participant.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Character"/>
        <rdfs:range rdf:resource="&rpg;Participant"/>
        <owl:inverseOf rdf:resource="&rpg;portrays"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#level -->

    <owl:ObjectProperty rdf:about="&rpg;level">
        <rdfs:label xml:lang="en">level</rdfs:label>
        <rdfs:comment xml:lang="en">Gives the level of a feature or temporary state.

The concept of a &apos;level&apos; is specific to a specific set of RPG rules and is specified further by sub-ontologies. Some features do not have a level associated with them.</rdfs:comment>
        <skos:note xml:lang="en">Use a level when it refers to a distinct category of abilities or state. If a simple numeric value is needed, use value.</skos:note>
        <rdfs:seeAlso rdf:resource="&rpg;value"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&rpg;Feature"/>
                    <rdf:Description rdf:about="&rpg;State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#portrays -->

    <owl:ObjectProperty rdf:about="&rpg;portrays">
        <rdfs:label xml:lang="en">portrays</rdfs:label>
        <rdfs:comment xml:lang="en">A participant portrays a character, speaking for the characters actions and controling the character&apos;s decsiions.</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Character"/>
        <rdfs:domain rdf:resource="&rpg;Participant"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#referenceMaterial -->

    <owl:ObjectProperty rdf:about="&rpg;referenceMaterial">
        <rdfs:label xml:lang="en">reference material</rdfs:label>
        <rdfs:comment xml:lang="en">A reference to a document or concept that contains additional information.</rdfs:comment>
        <rdfs:range rdf:resource="&foaf;Document"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#settingConcept -->

    <owl:ObjectProperty rdf:about="&rpg;settingConcept">
        <rdfs:label xml:lang="en">setting concept</rdfs:label>
        <rdfs:comment xml:lang="en">A concept that helps define the nature of a setting.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Setting"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#template -->

    <owl:ObjectProperty rdf:about="&rpg;template">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">template</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that the subject uses the object as a template.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;GameEntity"/>
        <rdfs:range rdf:resource="&rpg;GameEntity"/>
        <rdfs:range rdf:resource="&rpg;Template"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#toUnit -->

    <owl:ObjectProperty rdf:about="&rpg;toUnit">
        <rdfs:label xml:lang="en">to unit</rdfs:label>
        <rdfs:domain rdf:resource="&rpg;Conversion"/>
        <rdfs:range rdf:resource="&rpg;Unit"/>
    </owl:ObjectProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#unit -->

    <owl:ObjectProperty rdf:about="&rpg;unit">
        <rdfs:label xml:lang="en">unit</rdfs:label>
        <rdfs:comment xml:lang="en">The unit a quanity is expressed in.</rdfs:comment>
        <rdfs:range rdf:resource="&rpg;Unit"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&rpg;Attribute"/>
                    <rdf:Description rdf:about="&rpg;State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:ObjectProperty rdf:about="&terms;license">
        <rdfs:range rdf:resource="&terms;LicenseDocument"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/rights -->

    <owl:ObjectProperty rdf:about="&terms;rights">
        <rdfs:range rdf:resource="&terms;RightsStatement"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.travellerrpg.com/ontology/rpg#conversionRatio -->

    <owl:DatatypeProperty rdf:about="&rpg;conversionRatio">
        <rdfs:label xml:lang="en">conversion ratio</rdfs:label>
        <rdfs:comment xml:lang="en">The number to to units produced by one from unit.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Conversion"/>
        <rdfs:range rdf:resource="&owl;rational"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#fixedConversion -->

    <owl:DatatypeProperty rdf:about="&rpg;fixedConversion">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">fixed conversion</rdfs:label>
        <rdfs:comment xml:lang="en">This is a fixed conversion, defined by the nature of the units and, therefore, will not vary from setting to setting.</rdfs:comment>
        <rdfs:domain rdf:resource="&rpg;Conversion"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#value -->

    <owl:DatatypeProperty rdf:about="&rpg;value">
        <rdfs:label xml:lang="en">value</rdfs:label>
        <rdfs:comment xml:lang="en">The value of a feature or state.

Some features or states do not have a value associated with them, in which case the value is not needed.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&rpg;level"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&rpg;Feature"/>
                    <rdf:Description rdf:about="&rpg;State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.travellerrpg.com/ontology#DiceConventions -->

    <owl:Class rdf:about="http://data.travellerrpg.com/ontology#DiceConventions">
        <rdfs:label xml:lang="en">Dice Conventions</rdfs:label>
        <rdfs:subClassOf rdf:resource="&skos;ConceptScheme"/>
        <rdfs:comment xml:lang="en">Conventions for describing how die rolls are represented.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Actor -->

    <owl:Class rdf:about="&rpg;Actor">
        <rdfs:label xml:lang="en">Actor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;GameEntity"/>
        <rdfs:comment xml:lang="en">Something that can be interacted with within the game. 

Actors can be modelled within the game to various degrees of fidelity. For example, a player character may be more detailed than an animal that the character encounters.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Adventure -->

    <owl:Class rdf:about="&rpg;Adventure">
        <rdfs:label xml:lang="en">Adventure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;Setting"/>
        <rdfs:comment xml:lang="en">A story-level setting for a game, usually with a clear beginning, middle and end.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Amount -->

    <owl:Class rdf:about="&rpg;Amount">
        <rdfs:label xml:lang="en">Amount</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rpg;value"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;decimal"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rpg;unit"/>
                <owl:onClass rdf:resource="&rpg;Unit"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">A quantity of something</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Asset -->

    <owl:Class rdf:about="&rpg;Asset">
        <rdfs:label xml:lang="en">Asset</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;GameEntity"/>
        <rdfs:comment xml:lang="en">An object within the game that can be used or owned by an actor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Attribute -->

    <owl:Class rdf:about="&rpg;Attribute">
        <rdfs:label xml:lang="en">Attribute</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;Feature"/>
        <rdfs:comment xml:lang="en">A descriptive attribute of a character or item.</rdfs:comment>
        <skos:scopeNote xml:lang="en">Attributes are generally fixed things about a character or things which change very slowly. 

Attributes are generally central to the game system and are descibed via subclassing for a specific RPG, rather than by referencing individuals.</skos:scopeNote>
        <skos:example xml:lang="en">Examples are strength, size, education, as well as sex, hair colour, etc.</skos:example>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Campaign -->

    <owl:Class rdf:about="&rpg;Campaign">
        <rdfs:label xml:lang="en">Campaign</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;Setting"/>
        <rdfs:comment xml:lang="en">A series of events and adventures featuring a continuing set of chracters and settings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Character -->

    <owl:Class rdf:about="&rpg;Character">
        <rdfs:label xml:lang="en">Character</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;Actor"/>
        <rdfs:comment xml:lang="en">An RPG representation of a person or similar within-game creature capable of making its own decsions. </rdfs:comment>
        <skos:scopeNote xml:lang="en">Characters are usually modelled as something that could be controlled by a player, even if the character is not.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Conversion -->

    <owl:Class rdf:about="&rpg;Conversion">
        <rdfs:label xml:lang="en">Conversion</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;SettingState"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rpg;conversionRatio"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rpg;toUnit"/>
                <owl:onClass rdf:resource="&rpg;Unit"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rpg;fixedConversion"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rpg;fromUnit"/>
                <owl:onClass rdf:resource="&rpg;Unit"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A conversion between two units.</rdfs:comment>
        <rdfs:comment xml:lang="en">A date according to a system of dates.</rdfs:comment>
        <skos:scopeNote xml:lang="en">Generally, conversions are fixed and apply to the highest level setting. However, some conversions such as exchange rates may vary from setting to setting.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Currency -->

    <owl:Class rdf:about="&rpg;Currency">
        <rdfs:label xml:lang="en">Currency</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;Unit"/>
        <rdfs:comment xml:lang="en">A medium of exchange</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Date -->

    <owl:Class rdf:about="&rpg;Date">
        <rdfs:label xml:lang="en">Date</rdfs:label>
        <rdfs:subClassOf rdf:resource="&rpg;State"/>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#DateSystem -->

    <owl:Class rdf:about="&rpg;DateSystem">
        <rdfs:label xml:lang="en">Date System</rdfs:label>
        <rdfs:subClassOf rdf:resource="&skos;ConceptScheme"/>
        <rdfs:comment xml:lang="en">A real or fantasy date system.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.travellerrpg.com/ontology/rpg#Duration -->

    <owl:Class rdf:about="&rpg;Duration">
        <rdfs:label xml:lang="en">Duration</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
