     -->

    


    <!-- http://data.xeralabs.net/ontology/rpg#Actor -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Actor">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Asset"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Event"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Place"/>
        <rdfs:comment xml:lang="en">
      Something that can be interacted with within the game.

      Actors can be modelled within the game to various degrees of fidelity. For example, a player character may be more detailed than an animal that the character encounters.
    </rdfs:comment>
        <rdfs:label xml:lang="en">Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Adventure -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Adventure">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Setting"/>
        <rdfs:comment xml:lang="en">A story-level setting for a game, usually with a clear beginning, middle and end.</rdfs:comment>
        <rdfs:label xml:lang="en">Adventure</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Amount -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Amount">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.xeralabs.net/ontology/rpg#unit"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#value"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A quantity of something</rdfs:comment>
        <rdfs:label xml:lang="en">Amount</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Asset -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Asset">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Event"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Place"/>
        <rdfs:comment xml:lang="en">An object within the game that can be used or owned by an actor.</rdfs:comment>
        <rdfs:label xml:lang="en">Asset</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Attribute -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Attribute">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Feature"/>
        <rdfs:comment xml:lang="en">A descriptive attribute of a character or item.</rdfs:comment>
        <rdfs:label xml:lang="en">Attribute</rdfs:label>
        <skos:example xml:lang="en">Examples are strength, size, education, as well as sex, hair colour, etc.</skos:example>
        <skos:scopeNote xml:lang="en">
      Attributes are generally fixed things about a character or things which change very slowly.

      Attributes are generally central to the game system and are descibed via subclassing for a specific RPG, rather than by referencing individuals.
    </skos:scopeNote>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Campaign -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Campaign">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Setting"/>
        <rdfs:comment xml:lang="en">A series of events and adventures featuring a continuing set of chracters and settings.</rdfs:comment>
        <rdfs:label xml:lang="en">Campaign</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Character -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Character">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Actor"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Extra"/>
        <owl:disjointWith rdf:resource="http://data.xeralabs.net/ontology/rpg#Organisation"/>
        <rdfs:comment xml:lang="en">An RPG representation of a person or similar within-game creature capable of making its own decsions. </rdfs:comment>
        <rdfs:label xml:lang="en">Character</rdfs:label>
        <skos:scopeNote xml:lang="en">Characters are usually modelled as something that could be controlled by a player, even if the character is not.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Conversion -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Conversion">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#SettingState"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.xeralabs.net/ontology/rpg#fromUnit"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.xeralabs.net/ontology/rpg#toUnit"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.xeralabs.net/ontology/rpg#conversionRatio"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.xeralabs.net/ontology/rpg#fixedConversion"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A conversion between two units.</rdfs:comment>
        <rdfs:comment xml:lang="en">A date according to a system of dates.</rdfs:comment>
        <rdfs:label xml:lang="en">Conversion</rdfs:label>
        <skos:scopeNote xml:lang="en">Generally, conversions are fixed and apply to the highest level setting. However, some conversions such as exchange rates may vary from setting to setting.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Currency -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Currency">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
        <rdfs:comment xml:lang="en">A medium of exchange</rdfs:comment>
        <rdfs:label xml:lang="en">Currency</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Date -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Date">
        <rdfs:comment xml:lang="en">A date according to some calendar</rdfs:comment>
        <rdfs:label xml:lang="en">Date</rdfs:label>
        <skos:scopeNote xml:lang="en">The accuracy of a date is determined by its attributes, which may include year, month, day of year, day of month, time values, etc.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#DateSystem -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#DateSystem">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
        <rdfs:comment xml:lang="en">A real or fantasy date system.</rdfs:comment>
        <rdfs:label xml:lang="en">Date System</rdfs:label>
        <skos:altLabel xml:lang="en">Calendar</skos:altLabel>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#DiceConventions -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#DiceConventions">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:comment xml:lang="en">Conventions for describing how die rolls are represented.</rdfs:comment>
        <rdfs:label xml:lang="en">Dice Conventions</rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Document -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Document">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Item"/>
        <rdfs:comment xml:lang="en">An in-game document</rdfs:comment>
        <rdfs:label xml:lang="en">Document </rdfs:label>
    </owl:Class>
    


    <!-- http://data.xeralabs.net/ontology/rpg#Duration -->

    <owl:Class rdf:about="http://data.xeralabs.net/ontology/rpg#Duration">
        <rdfs:subClassOf rdf:resource="http://data.xeralabs.net/ontology/rpg#Amount"/>
        <rdfs:subClassOf>
