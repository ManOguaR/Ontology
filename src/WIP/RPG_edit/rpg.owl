<?xml version="1.0"?>
<rdf:RDF xmlns="http://data.xeralabs.net/ontology/rpg#"
     xml:base="http://data.xeralabs.net/ontology/rpg"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://data.xeralabs.net/ontology/rpg">
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <terms:creator rdf:resource="https://github.com/charvolant"/>
        <terms:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
        <rdfs:comment xml:lang="en">
      The RPG ontology contains abstract descriptions of common RPG concepts, such as Characters, Skills, NPCs etc. A specigic RPG will specialise these concepts to fit the RPG.
    </rdfs:comment>
        <rdfs:label xml:lang="en">Generic Role Playing Game Ontology</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.travellerrpg.com/CotI/Discuss/showthread.php?t=30041"/>
        <skos:scopeNote xml:lang="en">
      RPGs represent imaginary situations.

      Data available on the semantic web represent a wide variety of resources, from the wholely imaginary to the completely real.

      This ontology imports SKOS for knowledge descriptions and FOAF for player information. Otherwise, it keeps the real world at a distance, so that difficulties with software confusing a cyberpunk description of London and the real London are kept to a minimum.
    </skos:scopeNote>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.xeralabs.net/ontology/rpg#discriminator -->

    <owl:AnnotationProperty rdf:about="http://data.xeralabs.net/ontology/rpg#discriminator">
        <rdfs:comment xml:lang="en">What sub-property to use when determining template inhertiance. Template inheritance is, by default, decided on simple property identity. If there are properties that refer to a number of different sorts of things, then this property can be used to further discriminate based on the value of the property.</rdfs:comment>
        <rdfs:label xml:lang="en">discriminator</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
        <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    </owl:AnnotationProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#inherit -->

    <owl:AnnotationProperty rdf:about="http://data.xeralabs.net/ontology/rpg#inherit">
        <rdfs:comment xml:lang="en">This property is inherited by an object using a template. Inherit is normally assumed to be true for template statements. If a property is marked as inherit:false then resources using the template do not use these values. This can be useful for properties that more appropriately describe the class of things expressed by the template.</rdfs:comment>
        <rdfs:label xml:lang="en">inherit </rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    </owl:AnnotationProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#referenceMaterial -->

    <owl:AnnotationProperty rdf:about="http://data.xeralabs.net/ontology/rpg#referenceMaterial">
        <rdfs:comment xml:lang="en">A reference to a document or concept that contains additional information.</rdfs:comment>
        <rdfs:label xml:lang="en">reference material</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license">
        <rdfs:range rdf:resource="http://purl.org/dc/terms/LicenseDocument"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/rights -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/rights">
        <rdfs:range rdf:resource="http://purl.org/dc/terms/RightsStatement"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#Description -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#Description"/>
    


    <!-- http://www.w3.org/2002/07/owl#cardinality -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#cardinality"/>
    


    <!-- http://www.w3.org/2002/07/owl#qualifiedCardinality -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#qualifiedCardinality"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.xeralabs.net/ontology/rpg#amount -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#amount">
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Amount"/>
        <rdfs:comment xml:lang="en">An amount of some value</rdfs:comment>
        <rdfs:label xml:lang="en">amount</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#date -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#date">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#occurrenceAt"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Date"/>
        <rdfs:comment xml:lang="en">The date of an event or other occurance</rdfs:comment>
        <rdfs:label xml:lang="en">date</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#dateSystem -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#dateSystem">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#unit"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#DateSystem"/>
        <rdfs:comment xml:lang="en">The dating system used to record a date</rdfs:comment>
        <rdfs:label xml:lang="en">date system</rdfs:label>
        <skos:prefLabel xml:lang="en">calendar</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#duration -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#duration">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#amount"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Duration"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The duration of an event or process</rdfs:comment>
        <rdfs:label xml:lang="en">duration </rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#fromUnit -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#fromUnit">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Conversion"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
        <rdfs:label xml:lang="en">from unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasAttribute -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasAttribute">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#hasFeature"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Attribute"/>
        <rdfs:comment xml:lang="en">Gives the attributes that describe an actor</rdfs:comment>
        <rdfs:label xml:lang="en">has attribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasFaculty -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasFaculty">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#hasFeature"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Faculty"/>
        <discriminator rdf:resource="http://data.xeralabs.net/ontology/rpg#inFacultyType"/>
        <rdfs:comment xml:lang="en">Indicates that an actor has a specific faculty available</rdfs:comment>
        <rdfs:label xml:lang="en">has faculty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasFeature -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasFeature">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Feature"/>
        <rdfs:comment xml:lang="en">Indicates that a character possesses a certain feature.</rdfs:comment>
        <rdfs:label xml:lang="en">has feature</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasMass -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasMass">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#hasAttribute"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Mass"/>
        <rdfs:comment xml:lang="en">Gives the basic mass of a game item.</rdfs:comment>
        <rdfs:label xml:lang="en">mass</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasPrice -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasPrice">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#hasAttribute"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Price"/>
        <rdfs:comment xml:lang="en">
      Gives the price of an item.

      If left further unspecified, this generallty means the common, in-game price.
    </rdfs:comment>
        <rdfs:label xml:lang="en">price</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasSense -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasSense">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#hasFeature"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Sense"/>
        <discriminator rdf:resource="http://data.xeralabs.net/ontology/rpg#inSenseType"/>
        <rdfs:comment xml:lang="en">Indicates that an actor has a specific sense available</rdfs:comment>
        <rdfs:label xml:lang="en">has sense</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasSetting -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasSetting">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Setting"/>
        <rdfs:comment xml:lang="en">Indiicates that a gane entity, state or template applies to or comes from a particular setting.</rdfs:comment>
        <rdfs:label xml:lang="en">has setting</rdfs:label>
        <skos:scopeNote xml:lang="en">Settings themselves may have parent settings.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasSkill -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasSkill">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#hasFeature"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Skill"/>
        <discriminator rdf:resource="http://data.xeralabs.net/ontology/rpg#inSkillType"/>
        <rdfs:comment xml:lang="en">Indicates that an actor has a level of a specific skill</rdfs:comment>
        <rdfs:label xml:lang="en">has skill</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#hasState -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#hasState">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#State"/>
        <rdfs:comment xml:lang="en">Indicates that a game item is currently in a particular state</rdfs:comment>
        <rdfs:label xml:lang="en">is in state</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#inFacultyType -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#inFacultyType">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#inFeatureType"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Faculty"/>
        <rdfs:comment xml:lang="en">Gives the type of faculty that a faculty level refers to.</rdfs:comment>
        <rdfs:label xml:lang="en">in faculty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#inFeatureType -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#inFeatureType">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Feature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">Indicates that a feature is associated with a particular type of feature concept.</rdfs:comment>
        <rdfs:label xml:lang="en">in feature type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#inSenseType -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#inSenseType">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#inFeatureType"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Sense"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#SenseType"/>
        <rdfs:comment xml:lang="en">Gives the speciifc sense that a sense level gives the capability level of.</rdfs:comment>
        <rdfs:label xml:lang="en">in sense</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#inSkillType -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#inSkillType">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#inFeatureType"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Skill"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#SkillType"/>
        <rdfs:comment xml:lang="en">Indicates that a skill level is in a specific skill.</rdfs:comment>
        <rdfs:label xml:lang="en">in skill</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#isPortrayedBy -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#isPortrayedBy">
        <owl:inverseOf rdf:resource="http://data.xeralabs.net/ontology/rpg#portrays"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Character"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Participant"/>
        <rdfs:comment xml:lang="en">Indicates that a character is portrayed by a specific participant.</rdfs:comment>
        <rdfs:label xml:lang="en">is portrayed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#level -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#level">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Feature"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#occurrenceAt -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#occurrenceAt">
        <rdfs:comment xml:lang="en">When something has happened </rdfs:comment>
        <rdfs:label xml:lang="en">ocurrence  at</rdfs:label>
        <skos:scopeNote xml:lang="en">Sub-properties describe how an occurrence is measured: an absolute date, an interval since some other event etc.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#organisation -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#organisation">
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Organisation"/>
        <rdfs:comment xml:lang="en">Associated with a game organisation</rdfs:comment>
        <rdfs:label xml:lang="en">organisation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#parent -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#parent">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#related"/>
        <rdfs:comment xml:lang="en">Indicates a parent relationship between two game entities</rdfs:comment>
        <rdfs:label xml:lang="en">parent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#portrays -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#portrays">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Participant"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Character"/>
        <rdfs:comment xml:lang="en">A participant portrays a character, speaking for the characters actions and controling the character&apos;s decsiions.</rdfs:comment>
        <rdfs:label xml:lang="en">portrays</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#related -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#related">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#GameEntity"/>
        <rdfs:comment xml:lang="en">Indicates that two game entities are related in some way</rdfs:comment>
        <rdfs:label xml:lang="en">related </rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#settingConcept -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#settingConcept">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Setting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">A concept that helps define the nature of a setting.</rdfs:comment>
        <rdfs:label xml:lang="en">setting concept</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#template -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#template">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Template"/>
        <rdfs:comment xml:lang="en">Indicates that the subject uses the object as a template.</rdfs:comment>
        <rdfs:label xml:lang="en">template</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#toUnit -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#toUnit">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Conversion"/>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
        <rdfs:label xml:lang="en">to unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#unit -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#unit">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Amount"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Attribute"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Date"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Unit"/>
        <rdfs:comment xml:lang="en">The unit a quanity is expressed in.</rdfs:comment>
        <rdfs:label xml:lang="en">unit</rdfs:label>
    </owl:ObjectProperty>



  <!-- http://data.xeralabs.net/ontology/rpg#usesFeature -->

  <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#usesFeature">
    <rdfs:comment xml:lang="en">Indicates that a game activity uses a specific feature type. </rdfs:comment>
    <rdfs:comment xml:lang="en">Sub-properties describe what sort of feature is used by the activity. These may be a class or a reference to an instance, depdending on how the feature structure is organised.</rdfs:comment>
    <rdfs:label xml:lang="en">uses feature</rdfs:label>
  </owl:ObjectProperty>

  <!-- http://data.xeralabs.net/ontology/rpg#year -->

    <owl:ObjectProperty rdf:about="http://data.xeralabs.net/ontology/rpg#year">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Date"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://data.xeralabs.net/ontology/rpg#year"/>
                <owl:someValuesFrom rdf:resource="http://data.xeralabs.net/ontology/rpg#Date"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://data.xeralabs.net/ontology/rpg#Date"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.xeralabs.net/ontology/rpg#attribute -->

    <owl:DatatypeProperty rdf:about="http://data.xeralabs.net/ontology/rpg#attribute">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Amount"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Date"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#Feature"/>
                    <rdf:Description rdf:about="http://data.xeralabs.net/ontology/rpg#State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">An attribute of some state of affairs</rdfs:comment>
        <rdfs:label xml:lang="en">attribute</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#conversionDifference -->

    <owl:DatatypeProperty rdf:about="http://data.xeralabs.net/ontology/rpg#conversionDifference">
