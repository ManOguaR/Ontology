        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The amount to add (or subtract for negative values) from an amount of the from unit to conver to the to unit.</rdfs:comment>
        <rdfs:label xml:lang="en">conversion difference</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#conversionRatio -->

    <owl:DatatypeProperty rdf:about="http://data.xeralabs.net/ontology/rpg#conversionRatio">
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Conversion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The number to to units produced by one from unit.</rdfs:comment>
        <rdfs:label xml:lang="en">conversion ratio</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#dayOfMonth -->

    <owl:DatatypeProperty rdf:about="http://data.xeralabs.net/ontology/rpg#dayOfMonth">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#attribute"/>
        <rdfs:comment xml:lang="en">The day of an event within a month</rdfs:comment>
        <rdfs:label xml:lang="en">day of month</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#dayOfYear -->

    <owl:DatatypeProperty rdf:about="http://data.xeralabs.net/ontology/rpg#dayOfYear">
        <rdfs:subPropertyOf rdf:resource="http://data.xeralabs.net/ontology/rpg#attribute"/>
        <rdfs:domain rdf:resource="http://data.xeralabs.net/ontology/rpg#Date"/>
        <rdfs:comment xml:lang="en">The day within a year of a date</rdfs:comment>
        <rdfs:label xml:lang="en">day of year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.xeralabs.net/ontology/rpg#fixedConversion -->

    <owl:DatatypeProperty rdf:about="http://data.xeralabs.net/ontology/rpg#fixedConversion">
